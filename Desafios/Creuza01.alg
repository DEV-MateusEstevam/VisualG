algoritmo "desafio097"
var
       ano_atual, ano_nasc, idade: inteiro
inicio
       escreval("Calculanfdo idade: ")
       escreval()
       escreva("Em que ano nós estamos? ")
       leia(ano_atual)
       escreval()
       escreva("Em que ano você nasceu? ")
       leia(ano_nasc)
       escreval()
       idade <- ano_atual - ano_nasc
       escreval("Minha idade é ",idade," anos")
fimalgoritmo
