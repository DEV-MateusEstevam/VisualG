algoritmo "desafio022"
var
     N, C, S: inteiro
inicio
      escreval("Somador usando função PARA:")
      escreval()
       S <- 0
       para C <- 1 ate 5 faca
                escreva("Digite um valor: ")
                leia(N)
                escreval()
                S <- S + N
       FimPara
       Escreval("A soma dos valores é ",S)
fimalgoritmo
