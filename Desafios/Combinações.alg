algoritmo "desafio082"
var
   contador1, contador2: inteiro
inicio
      escreval("Realizando Combinações Numéricas: ")
      escreval()
      para contador1 := 1 ate 3 faca // <- é a mesma coisa que :=(recebe)
           para contador2 := 1 ate 3 faca
                escreval(contador1," ",contador2)
           fimpara
      fimpara
      
fimalgoritmo
