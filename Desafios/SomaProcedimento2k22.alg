algoritmo "FazsomaProcedimento"
var
   X, Y: inteiro
Procedimento soma(A, B:inteiro)
inicio
      escreval("Recebi o valor",X)
      escreval("Recebi o valor",Y)
      escreval("A soma entre os dois é",A + B)
fimprocedimento
inicio
      escreval("Somando usando Procedimentos:")
      escreval()
      X <- 5
      Y <- 3
      soma(X, Y)
fimalgoritmo
