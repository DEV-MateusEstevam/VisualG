algoritmo "conteate10"
// Função :
// Autor :
// Data : 27/06/2022
// Seção de Declarações 
var
   contador: inteiro
inicio
      contador <- 10
      enquanto (contador <= 100) faca
               escreval("contando ...",contador)
               contador <- contador + 10
      fimenquanto
      escreval("Terminei de contar!")
fimalgoritmo
