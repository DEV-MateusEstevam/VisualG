algoritmo "desafio085"
var
   contador: inteiro
inicio
      escreval("CONTADOR: ")
      escreval()
      contador <- 10
      enquanto (contador <= 100) faca
               escreval("contando ...",contador)
               contador <- contador + 10
      fimenquanto
      escreval()
      escreval("Terminei de contar!")
fimalgoritmo
