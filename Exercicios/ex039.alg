algoritmo "ex039"
var
   contagem: inteiro
inicio
      escreval("39) Faça um algoritmo que mostre na tela a seguinte contagem:")
      escreval()
      contagem <- 10
      enquanto (contagem >= 3) faca
               escreva(contagem," ")
               contagem <- contagem - 1
      fimenquanto
      escreval("Acabou!")
fimalgoritmo
