algoritmo "ex064"
var
   contador: inteiro
inicio
      escreval("Desenvolva um programa usando a estrutura “para” que mostre na tela a")
      escreval("seguinte contagem: 0 5 10 15 20 25 30 35 40 Acabou!")
      escreval()
      para contador <- 0 ate 40 passo 5 faca
           escreva(contador)
      fimpara
      escreval(" Acabou!")
fimalgoritmo
