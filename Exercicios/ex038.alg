algoritmo "ex038"
var
    contagem: inteiro
inicio
      escreval("Escreva um programa que mostre na tela a seguinte contagem:")
      escreval()
      contagem <- 6
      enquanto (contagem <= 11) faca
               escreva(contagem," ")
               contagem <- contagem + 1
      fimenquanto
      escreval("Acabou.")
fimalgoritmo
