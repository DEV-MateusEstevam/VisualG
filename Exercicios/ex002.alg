algoritmo "ex002"
var
    nome: caractere
inicio
      escreval("2) Faça um programa que leia o nome de uma pessoa e mostre uma mensagem de boas vindas para ela:")
      escreval()
      escreva("Qual é o seu nome? ")
      leia(nome)
      escreval()
      escreval("Olá ",nome," é um prazer te conhecer!")
fimalgoritmo
